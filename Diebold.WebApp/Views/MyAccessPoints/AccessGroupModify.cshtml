    function GetAccessGroupModifyCallback(ResultSet) {
        UnBlockElement("dvCardholderModifyContent");
        if (ResultSet != null && ResultSet != undefined) {
            if (ResultSet.Message != null && ResultSet.Message != undefined && ResultSet.Message != "") {
                fnShowAlertInfo(ResultSet.Message);
            }
            else {
                if (ResultSet == "OK") {
                    fnShowAlertInfo('Access group modified successfully.');
                    $('#cboCHModifyAG').val('');
                    document.getElementById('my_overlay').style.visibility = 'hidden';
                    document.getElementById('dvModifyAccessGroups').style.visibility = 'hidden';
                    fnCloseModifyAccessGroup();
                }
            }
        }
    }

    var isValidReader = false;
    function CheckReadersItems(aId) {
        var collection = document.getElementById(aId).getElementsByTagName('INPUT');
