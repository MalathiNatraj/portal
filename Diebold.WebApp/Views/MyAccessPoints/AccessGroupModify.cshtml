                        <strong>Name:(*)</strong></p>
                </div>                                
                <div class="my-access-field bottom">                                       
                     @(Html.Kendo().ComboBox()
                        .Name("cboCHModifyAG")
                        .Placeholder("-- Select --")
                        .HtmlAttributes(new { style = "width:100%" })
                        .DataTextField("Name")
                        .DataValueField("Id")
                        .BindTo(Model.AccessGroupList)
                        .Suggest(true)
                        .Events(events => events.Change("ModifyAGGroup_SelectionChanged"))
                        )
                </div>
                </div>
                <div class="clear">
                </div>
                <div id="dvModifyAGContent" style="display:none;">
