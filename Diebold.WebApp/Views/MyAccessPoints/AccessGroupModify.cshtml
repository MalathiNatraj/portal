            if (collection[x].type.toUpperCase() == 'CHECKBOX') {
                if (collection[x].checked == true) {
                    isValidReader = true;
                    break;
                }
                else {
                    isValidReader = false;
                }
            }
        }
        return isValidReader;
    }
    
